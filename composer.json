{
  "name": "perfocard/flog",
  "description": "Laravel wrapper for the Flog binary",
  "type": "library",
  "require": {
    "php": ">=8.4",
    "illuminate/support": "^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0.1",
    "orchestra/testbench": "^8.14|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Perfocard\\Flog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Perfocard\\Flog\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Perfocard\\Flog\\FlogServiceProvider"
      ]
    }
  }
}
